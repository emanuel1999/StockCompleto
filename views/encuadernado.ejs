<div class="container">
  <a href="/create" class="btn btn-outline-primary mt-4 " style="font-size: 24px;">Create</a>
  <a href="/" class="btn btn-outline-primary mt-4 " style="font-size: 24px;">Inicio</a>
  <a href="/impresion" class="btn btn-outline-primary mt-4 " style="font-size: 24px;">Area De Impresion</a>
  <a href="/corte" class="btn btn-outline-primary mt-4 " style="font-size: 24px;">Area De Corte</a>
  <a href="/encuadernado" class="btn btn-outline-primary mt-4 " style="font-size: 24px;">Area De Encuadernado</a>
  <a href="/tapa" class="btn btn-outline-primary mt-4 " style="font-size: 24px;">Area De Tapa</a>
  <a href="/reportes" class="btn btn-outline-primary mt-4 " style="font-size: 24px;">Area De Reportes</a>
  <a href="/createReportes" class="btn btn-outline-primary mt-4 " style="font-size: 24px;">Create Reporte</a>
  <div><a style="font-size: 24px;text-align: center;">Encuadernado</a></div>
  <table class="table table-bordered table-striped text-center mt-4 " id="example">
    <thead>
      <tr class="bg-primary text-white">
      <th scope="col">Prioridad</th>
      <th scope="col">Estado Encuadernado</th>
      <th scope="col">Libro</th>
      <th scope="col">Tama√±o Cerrado</th>
      <th scope="col">Acabado Gral</th>
      <th scope="col">Solapa</th>
      <th scope="col">Accion</th>
      </tr>
    </thead>
    <tbody>
      <% results.forEach((libros)=> { %>
        <tr>
       <td><%=libros.prioridad%></td> 
       <%if(libros.estadoencuadernado=='Sin Entregar'){%>
        <td style="background-color:#FF3636;color:black;"><a href="editEncuadernado/<%=libros.id%>"><%=libros.estadoencuadernado%></a></td> 
       <%}%>
       <%if(libros.estadoencuadernado=='En Proceso'){%>
        <td style="background-color:#8AF333;color:black;"><a href="editEncuadernado/<%=libros.id%>"><%=libros.estadoencuadernado%></a></td> 
       <%}%>
       <%if(libros.estadoencuadernado=='Entregado'){%>
        <td style="background-color:#00C9FF;color:black;"><a href="editEncuadernado"><%=libros.estadoencuadernado%></a></td> 
       <%}%>
       <td><%=libros.libro%></td> 
       <td><%=libros.cerrado%></td>
       <td><%=libros.acabadolibro%></td>
       <td><%=libros.solapa%></td>
       <td>
         <a href="/edit/<%=libros.id%>" class="btn btn-outline-info">Editar</a>
         <a href="/delete/<%=libros.id%>" class="btn btn-outline-danger">Borrar</a>
       </td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>

