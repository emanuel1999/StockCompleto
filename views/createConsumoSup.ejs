<div class="container mt-4" >
    <div class="card border-secondary" >
    <div class="card-header">Editar Libro</div>
    <div class="card-body">
     
       
        <form action="/saveConsumoSup" method="POST">
          <% if (libros) { %>
            <div class="mb-3">
              <label for="papel" class="form-label">Papel</label>
              <select name="papel" id="papel" class="form-select" tabindex="2" required>
                <option value="">Selecione un papel</option>
                <% libros.forEach((libro)=> { %>
                  <% for(var i =0; i<1; i++){%>
                     <option value=<%=libro.papel%> > <%=libro.papel%> </option> 
                     <% } %>
                     <% }) %>
                
              </select>    
          </div>
          <div class="mb-3">
            <label for="provedor" class="form-label">Proveedor</label>
            <select name="provedor" id="provedor" class="form-select" tabindex="2" required>
              <option value="">Selecione un proveedor</option>
              <option value="Celulosa_Argentina">Celulosa_Argentina</option>
              <option value="Stenfar/Susano">Stenfar/Susano</option>
              <option value="Dimagraf">Dimagraf</option>
              <option value="Valendam">Valendam</option>
              <option value="RICOH">RICOH</option>
              <option value="OSA">OSA</option>
            </select> 
            </div>
         
     
<%}%>


   
