<% if (libros){ %>
<div class="container mt-4" >
    <div class="card border-secondary" >
    <div class="card-header">Editar Libro</div>
    <div class="card-body">
        <form action="/update" method="POST">
            <input hidden type="text" name="id" id="id" value="<%= libros.id %>">
            <div class="mb-3">
                <label for="prioridad" class="form-label">Prioridad</label>
                <input type="number" name="prioridad" id="prioridad" class="form-control" tabindex="1" value="<%= libros.prioridad %>">
            </div>
            <div class="mb-3">
                <label for="estado" class="form-label" >Estado</label>
                <select name="estado" id="estado" class="form-select" tabindex="2">
                  <option <%= libros.estado =='Sin Entregar' ? "selected" : '' %> value="Sin Entregar">Sin Entregar</option>
                  <option <%= libros.estado =='En Proceso' ? "selected" : '' %> value="En Proceso">En Proceso</option>
                  <option <%= libros.estado =='Entregado' ? "selected" : '' %> value="Entregado">Entregado</option>
                </select>
            </div>
            <div class="mb-3">
              <label for="editorial" class="form-label">Editorial</label>
              <input type="text" name="editorial" id="editorial" class="form-control" tabindex="1" required value="<%= libros.editorial %>">
          </div>
            <div class="mb-3">
                <label for="libro" class="form-label">Libro</label>
                <input type="text" name="libro" id="libro" class="form-control" tabindex="1" value="<%= libros.libro %>">
            </div>
            <div class="mb-3">
                <label for="fechaentrega" class="form-label">Fecha De Entrega</label>
                <input type="date" name="fechaentrega" id="fechaentrega" class="form-control" tabindex="1" value="<%= libros.fechaentrega %>">
            </div>
            <div class="mb-3">
                <label for="fechapentrega" class="form-label">Fecha Probable De Entrega</label>
                <input type="date" name="fechapentrega" id="fechapentrega" class="form-control" tabindex="1" value="<%= libros.fechapentrega %>" >
            </div>
            <div class="mb-3">
                <label for="pliego" class="form-label">Pliego</label>
                <select name="pliego" id="pliego" class="form-select" tabindex="2">
                  <option <%= libros.pliego =='Producto' ? "selected" : '' %> value="Producto">Producto</option>
                  <option <%= libros.pliego =='Libro' ? "selected" : '' %> value="Libro">Libro</option>
                  <option <%= libros.pliego =='Tapa' ? "selected" : '' %> value="Tapa">Tapa</option>
                </select>
            </div>
            <div class="mb-3">
                <label for="tirajeCondemasia" class="form-label">Tiraje Con Demasia</label>
                <input type="number" name="tirajeCondemasia" id="tirajeCondemasia" class="form-control" tabindex="1" value="<%= libros.tirajeCondemasia %>">
            </div>
            
            <div class="mb-3">
                <label for="tirajecliente" class="form-label" >Se Entrega Al Cliente</label>
                <input type="number" name="tirajecliente" id="tirajecliente" class="form-control" tabindex="1" value="<%= libros.tirajecliente %>">
            </div>
            <div class="mb-3">
                <label for="tirajeosa" class="form-label">Se Entrega A OSA</label>
                <input type="number" name="tirajeosa" id="tirajeosa" class="form-control" tabindex="1" value="<%= libros.tirajeosa %>">
            </div>
            
            <div class="mb-3"><label for="cerrado" class="form-label" style="text-align: center;">Seccion Orden De Trabajo</label></div>
            <div class="mb-3">
              <label for="tacocorte" class="form-label">Taco A Cortar </label>
              <input type="text" name="tacocorte" id="tacocorte" class="form-control" tabindex="1" value="<%= libros.tacocorte %>">
            </div>
            <div class="mb-3">
              <label for="volumentaco" class="form-label">Volumen De Taco </label>
              <input type="text" name="volumentaco" id="volumentaco" class="form-control" tabindex="1" value="<%= libros.volumentaco %>">
            </div>
            <div class="mb-3">
              <label for="tamañopdf" class="form-label">Tamaño De PDF A Imprimir</label>
              <input type="text" name="tamañopdf" id="tamañopdf" class="form-control" tabindex="1" value="<%= libros.tamañopdf %>" >
            </div>
            <div class="mb-3">
              <label for="provedortapa" class="form-label">Provedor Tapa</label>
              <input type="text" name="provedortapa" id="provedortapa" class="form-control" tabindex="1" value="<%= libros.provedortapa %>" >
            </div>
            <div class="mb-3">
              <label for="provedorinterior" class="form-label">Provedor Interior</label>
              <input type="text" name="provedorinterior" id="provedorinterior" class="form-control" tabindex="1" value="<%= libros.provedorinterior %>">
            </div>
            <div class="mb-3">
              <label for="cerrado" class="form-label">Cerrado (en mm.)</label>
              <input type="text" name="cerrado" id="cerrado" class="form-control" tabindex="1" value="<%= libros.cerrado %>">
            </div>
          
            <div class="mb-3">
              <label for="paginatapa" class="form-label">Páginas Interior + Tapa</label>
              <input type="text" name="paginatapa" id="paginatapa" class="form-control" tabindex="1" value="<%= libros.paginatapa %>">
            </div>
          
            <div class="mb-3">
              <label for="color" class="form-label">Color / ByN	</label>
              <input type="text" name="color" id="color" class="form-control" tabindex="1" value="<%= libros.color %>">
             </div>
            <div class="mb-3">
              <label for="up" class="form-label">UPs</label>
              <input type="text" name="up" id="up" class="form-control" tabindex="1" value="<%= libros.up %>" >
            </div>            
            <div class="mb-3">
              <label for="encuadernacion" class="form-label">Encuadernación</label>
              <input type="text" name="encuadernacion" id="encuadernacion" class="form-control" tabindex="1" value="<%= libros.encuadernacion %>" >
            </div>
          
            <div class="mb-3">
              <label for="solapa" class="form-label">Solapas</label>
              <select name="solapa" id="solapa" class="form-select" tabindex="2">
                <option <%= libros.solapa =='Sin Solapa' ? "selected" : '' %> value="Sin Solapa">Sin Solapa</option>
                <option <%= libros.solapa =='Con Solapa' ? "selected" : '' %> value="Con Solapa">Con Solapa</option>
                </select>
             </div> 
            <div class="mb-3">
              <label for="entregaarchivos" class="form-label">Entrega de Archivos</label>
              <input type="date" name="entregaarchivos" id="entregaarchivos" class="form-control" tabindex="1" value="<%= libros.entregaarchivos %>">
            </div>
             <div class="mb-3">
              <label for="tirada" class="form-label">Tirada</label>
              <input type="date" name="tirada" id="tirada" class="form-control" tabindex="1"value="<%= libros.tirada %>" >
            </div>            
            <div class="mb-3">
              <label for="despachofinal" class="form-label">Despacho Final</label>
              <input type="date" name="despachofinal" id="despachofinal" class="form-control" tabindex="1"  value="<%= libros.despachofinal %>">
            </div>  
            <div class="mb-3">
              <label  class="form-label">Seccion Taco</label>
              </div>
              <div class="mb-3">
                <label for="tipoimpresiontaco" class="form-label">Tipo De Impresion Taco </label>
                <input type="text" name="tipoimpresiontaco" id="tipoimpresiontaco" class="form-control" tabindex="1" value="<%= libros.tipoimpresiontaco %>" >
              </div> 
              <div class="mb-3">
                <label for="estadoimpresion" class="form-label">Estado De Impresion Taco </label>
                <select name="estadoimpresion" id="estadoimpresion" class="form-select" tabindex="2">
                  <option <%= libros.estadoimpresion =='Sin Entregar' ? "selected" : '' %> value="Sin Entregar">Sin Entregar</option>
                  <option <%= libros.estadoimpresion =='En Proceso' ? "selected" : '' %> value="En Proceso">En Proceso</option>
                  <option <%= libros.estadoimpresion =='Entregado' ? "selected" : '' %> value="Entregado">Entregado</option>
                </select>
              </div>
              <div class="mb-3">
                <label for="estadocorte" class="form-label">Estado De Corte Del Taco </label>
                <select name="estadocorte" id="estadocorte" class="form-select" tabindex="2">
                  <option <%= libros.estadocorte =='Sin Entregar' ? "selected" : '' %> value="Sin Entregar">Sin Entregar</option>
                  <option <%= libros.estadocorte =='En Proceso' ? "selected" : '' %> value="En Proceso">En Proceso</option>
                  <option <%= libros.estadocorte =='Entregado' ? "selected" : '' %> value="Entregado">Entregado</option>
                </select>
              </div>        
            <div class="mb-3">
              <label for="pliegoop" class="form-label">Taco </label>
              <input type="text" name="pliegoop" id="pliegoop" class="form-control" tabindex="1" value="<%= libros.pliegoop %>" >
            </div>
            <div class="mb-3">
              <label for="tipopapel" class="form-label">Tipo De Papel</label>
              <input type="text" name="tipopapel" id="tipopapel" class="form-control" tabindex="1" value="<%= libros.tipopapel %>" >
            </div>
           
            <div class="mb-3">
              <label for="hojas" class="form-label">Tamaño y Gramaje</label>
              <input type="text" name="hojas" id="hojas" class="form-control" tabindex="1" value="<%= libros.hojas %>">
             </div>
            <div class="mb-3">
              <label for="hojaskgcolores" class="form-label">Total paginas, KG , Colores</label>
              <input type="text" name="hojaskgcolores" id="hojaskgcolores" class="form-control" tabindex="1" value="<%= libros.hojaskgcolores %>">
            </div>  
            <div class="mb-3">
              <label  class="form-label">Seccion Tapa</label>
              </div>
            <div class="mb-3">
              <label for="pliegoop1" class="form-label">Pliego Tapa</label>
              <input type="text" name="pliegoop1" id="pliegoop1" class="form-control" tabindex="1" value="<%= libros.pliegoop1 %>" >
            </div> 
            <div class="mb-3">
              <label for="tipopapel1" class="form-label">Tipo De Papel</label>
              <input type="text" name="tipopapel1" id="tipopapel1" class="form-control" tabindex="1" value="<%= libros.tipopapel1 %>" >
            </div>
            <div class="mb-3">
              <label for="hoja1" class="form-label">Tamaño y Gramaje</label>
              <input type="text" name="hoja1" id="hoja1" class="form-control" tabindex="1" value="<%= libros.hoja1 %>" >
            </div>  
            <div class="mb-3">
              <label for="hojaskgcolores1" class="form-label">Total paginas, KG , Colores</label>
              <input type="text" name="hojaskgcolores1" id="hojaskgcolores1" class="form-control" tabindex="1" value="<%= libros.hojaskgcolores1 %>">
             </div>
            <div class="mb-3">
              <label for="acabadolibro" class="form-label">Acabado Gral</label>
              <input type="text" name="acabadolibro" id="acabadolibro" class="form-control" tabindex="1" value="<%= libros.acabadolibro %>" >
            </div>
            <div class="mb-3">
              <label for="acabadotapa" class="form-label">Acabado Tapa:</label>
              <input type="text" name="acabadotapa" id="acabadotapa" class="form-control" tabindex="1" value="<%= libros.acabadotapa %>" >
             </div>
            <div class="mb-3">
              <label for="descripcion" class="form-label">Descripcion</label>
              <input type="text" name="descripcion" id="descripcion" class="form-control" tabindex="1" value="<%= libros.descripcion %>" >
            </div>

            <button type="submit" class="btn btn-primary" tabindex="5">Guardar</button>
            <a href="/" class="btn btn-secondary">Cancelar</a>
        </form>
    </div>
</div>
</div>
<%}%>

   
